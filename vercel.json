{
  "version": 2,
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": "apps/frontend/.next",
  "builds": [
    {
      "src": "apps/frontend/next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/frontend/$1"
    }
  ],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/frontend/",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
